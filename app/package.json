{
  "name": "learning-genie",
  "productName": "learning-genie",
  "version": "1.0.0",
  "description": "learning-genie",
  "main": "index.js",
  "scripts": {
    "start": "electron index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . learning-genie --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Learning Genie\"",    
    "package-linux": "electron-packager . learning-genie --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
    
  },
  "author": "Ankit chawla ankitchawla777@gmail.com",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "fs": "0.0.1-security",
    "jquery": "^3.3.1",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "electron": "^1.8.2",
    "electron-packager": "^12.0.1"
  }
}
